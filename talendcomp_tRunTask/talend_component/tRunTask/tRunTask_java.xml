<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT>
	<HEADER AUTHOR="cimt AG" COMPATIBILITY="ALL"
		DATA_AUTO_PROPAGATE="false" PLATEFORM="ALL" RELEASE_DATE="20140703A"
		SERIAL="" STARTABLE="true" STATUS="PROD" VERSION="1.8">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
	    <FAMILY>System</FAMILY>
		<FAMILY>Orchestration</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
	    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
	    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER FIELD="TEXT" NAME="TAC_URL" NUM_ROW="10" REQUIRED="true">
			<DEFAULT>"http://localhost:8080/org.talend.administrator"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="TAC_USER" NUM_ROW="20" REQUIRED="true">
			<DEFAULT>"admin@company.com"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="TAC_PASSWD" NUM_ROW="20" REQUIRED="true">
			<DEFAULT>"admin"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="USE_TASK_NAME" NUM_ROW="30" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="TASK_NAME" NUM_ROW="40" REQUIRED="true" SHOW_IF="USE_TASK_NAME == 'true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="TASK_ID" NUM_ROW="40" REQUIRED="true" SHOW_IF="USE_TASK_NAME == 'false'">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
	    <PARAMETER NAME="PROCESS" FIELD="PROCESS_TYPE" NUM_ROW="50" />
	    <PARAMETER NAME="CONTEXT_PARAMS" FIELD="TABLE" NUM_ROW="55" NB_LINES="8">
	      <ITEMS>
	        <ITEM NAME="NAME" FIELD="CONTEXT_PARAM_NAME_LIST" />
	        <ITEM NAME="VALUE" FIELD="TEXT" />
	      </ITEMS>
	    </PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="ASYNCHRONOUS_RUN" NUM_ROW="60" REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="WAIT_UNTIL_END" NUM_ROW="60" REQUIRED="true">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="WAIT_UNTIL_RUNNING_CHECK_CYCLE" REQUIRED="true" NUM_ROW="70" SHOW_IF="ASYNCHRONOUS_RUN == 'true'">
			<DEFAULT>200</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="WAIT_UNTIL_RUNNING_TIMEOUT" REQUIRED="true" NUM_ROW="70" SHOW_IF="ASYNCHRONOUS_RUN == 'true'">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="WAIT_UNTIL_FINISHED_CHECK_CYCLE" REQUIRED="true" NUM_ROW="70" SHOW_IF="ASYNCHRONOUS_RUN == 'true'">
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="CHECK_TASK_NOT_RUNNING_SIMULTANEOUSLY" NUM_ROW="72" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	    <PARAMETER NAME="TASK_NOT_RUNNING_SIMULTANEOUSLY" FIELD="TABLE" NUM_ROW="76" NB_LINES="8" SHOW_IF="CHECK_TASK_NOT_RUNNING_SIMULTANEOUSLY == 'true'">
	      <ITEMS>
	        <ITEM NAME="TASK_NAME" FIELD="TASK_NAME" />
	      </ITEMS>
	    </PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DIE_ON_ERROR" NUM_ROW="80" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER FIELD="CHECK" NAME="DEBUG" NUM_ROW="10" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="ALLOW_GENERATING" NUM_ROW="20" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="cimt.talendcomp.tacws" MODULE="cimt.talendcomp.tacws-1.8.0-jar-with-dependencies.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="TASK_ID" TYPE="id_String" />
		<RETURN AVAILABILITY="AFTER" NAME="RUN_DURATION" TYPE="id_Long" />
		<RETURN AVAILABILITY="AFTER" NAME="RETURN_CODE" TYPE="id_Integer" />
	</RETURNS>
</COMPONENT>
